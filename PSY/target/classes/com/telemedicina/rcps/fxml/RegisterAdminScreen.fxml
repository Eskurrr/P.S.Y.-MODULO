<GridPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.modulo.psy.main.controller.RegisterAdminController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="302.0" minWidth="10.0" prefWidth="302.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="306.0" minWidth="10.0" prefWidth="298.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <VBox alignment="CENTER" minHeight="0.0" minWidth="0.0" prefHeight="354.0" prefWidth="566.0" spacing="25.0" style="-fx-background-color: WHITE;">
            <children>
                <MFXTextField fx:id="adminName" floatMode="ABOVE" floatingText="Name" maxHeight="40.0" maxWidth="160.0" minHeight="20.0" minWidth="0.0" prefHeight="25.0" prefWidth="160.0" />
                <MFXTextField fx:id="adminNumber" floatMode="ABOVE" floatingText="Phone number" maxHeight="40.0" maxWidth="160.0" minHeight="20.0" minWidth="0.0" prefHeight="25.0" prefWidth="160.0" />
                <MFXTextField fx:id="adminMail" floatMode="ABOVE" floatingText="Email" maxHeight="40.0" maxWidth="160.0" minHeight="20.0" minWidth="0.0" prefHeight="25.0" prefWidth="160.0" />
                <MFXPasswordField fx:id="adminPassword" floatMode="ABOVE" floatingText="Password" maxHeight="40.0" maxWidth="160.0" minHeight="20.0" minWidth="0.0" prefHeight="25.0" prefWidth="240.0" />
                <MFXPasswordField fx:id="adminConfirmP" floatMode="ABOVE" floatingText="Confirm Password" maxHeight="40.0" maxWidth="160.0" minHeight="20.0" minWidth="0.0" prefHeight="25.0" prefWidth="160.0" />
            <Label fx:id="adminWarning" alignment="CENTER" contentDisplay="CENTER" />
                <MFXButton fx:id="adminRegister" onAction="#adminRegister_click" rippleColor="#dc05f8" style="-fx-background-color:#66E0E0;" text="Sign Up" />
            </children>
        </VBox>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
         <children>
            <ImageView fitHeight="219.0" fitWidth="269.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../images/RCPS-IM.png" />
            </ImageView>
         </children>
      </VBox>
    </children>
</GridPane>
